{
  "name": "chatgpt-api-pool",
  "type": "module",
  "version": "0.5.0",
  "description": "Build your own ChatGPT with a request pool.",
  "author": "alexzhang1030",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzhang1030/chatgpt-api-pool.git"
  },
  "keywords": [
    "chatgpt",
    "api",
    "pool"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit \"chore: release v%s\" --push --tag && pnpm publish",
    "build": "unbuild"
  },
  "dependencies": {
    "chatgpt": "^5.0.6",
    "consola": "^2.15.3",
    "nodemailer": "^6.9.1"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.36.0",
    "@types/node": "^18.14.6",
    "@types/nodemailer": "^6.4.7",
    "bumpp": "^9.0.0",
    "eslint": "^8.35.0",
    "lint-staged": "^13.1.2",
    "p-queue": "^7.3.4",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vitest": "^0.29.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,json,md,yaml,yml}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-ts"
  }
}
